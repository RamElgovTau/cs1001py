<?xml version="1.0"?>
<functions>
    <print_rectangle sign="1a">
        <test  sign="1a" grade="4">
            <input id="1">3, 4</input>
            <output id="1">"****\n*  *\n****"</output>
            <input id="2">3, 3</input>
            <output id="2">"***\n* *\n***"</output>
            <input id="3">10, 3</input>
            <output id="3">"***\n* *\n* *\n* *\n* *\n* *\n* *\n* *\n* *\n***"</output>
        </test>
    </print_rectangle>
    <x_o_winner sign="1b">
        <test  sign="1b1" grade="4">
            <input id="1">["eee", "xxx", "eoo"]</input>
            <output id="1">"x"</output>
            <input id="2">["eox", "oxe", "xeo"]</input>
            <output id="2">"x"</output>
            <input id="3">["eox", "eox", "xoe"]</input>
            <output id="3">"o"</output>
        </test>
        <test  sign="1b2" grade="1">
            <input id="1">["xxo", "oox", "xox"]</input>
            <output id="1">"no winner"</output>
        </test>
    </x_o_winner>
    <valid_braces sign="1c">
        <test  sign="1c1" grade="5">
            <input id="1">"(^{%}.6)"</input>
            <output id="1">True</output>
            <input id="2">"({.x@[@]})"</input>
            <output id="2">True</output>
            <input id="3">"(.{[.aaasd.]R)"</input>
            <output id="3">False</output>
        </test>
        <test  sign="1c2" grade="1">
            <input id="1">"[()*{*[o](  )/}]"</input>
            <output id="1">True</output>
            <input id="2">"[  ( )1{2[`](``}]"</input>
            <output id="2">False</output>
        </test>
    </valid_braces>

    <test_coin sign="2a">
        <test  sign="2a" grade="3">
            <input id="1">None</input>
            <output id="1">True</output>
        </test>
    </test_coin>
    <test_roll_dice sign="2b">
        <test  sign="2b" grade="2">
            <input id="1">6</input>
            <output id="1">True</output>
            <input id="2">10</input>
            <output id="2">True</output>
        </test>
    </test_roll_dice>
    <test_roulette sign="2c">
        <test  sign="2c" grade="2">
            <input id="1">"odd"</input>
            <output id="1">True</output>
            <input id="2">"even"</input>
            <output id="2">True</output>
        </test>
    </test_roulette>
    <test_roulette_repeat sign="2d">
        <test  sign="2d" grade="2">
            <input id="1">3, 10000</input>
            <output id="1">True</output>
            <input id="2">6, 10000</input>
            <output id="2">True</output>
        </test>
    </test_roulette_repeat>
    <test_shuffle_list sign="2e">
        <test  sign="2e" grade="4">
            <input id="1">[1, 2, 3]</input>
            <output id="1">True</output>
            <input id="2">[1, 2, 3, 4]</input>
            <output id="2">True</output>
            <input id="2">[1]</input>
            <output id="2">True</output>
        </test>
    </test_shuffle_list>
    <test_avg_count_steps sign="2f">
        <test  sign="2f" grade="2">
            <input id="1">9</input>
            <output id="1">True</output>
        </test>
    </test_avg_count_steps>
    <test_count_steps_2dim sign="2g">
        <test  sign="2g1" grade="1">
            <input id="1">9</input>
            <output id="1">True</output>
            <input id="2">25</input>
            <output id="2">True</output>
        </test>
    </test_count_steps_2dim>

    <inc sign="3a">
        <test  sign="3a" grade="4">
            <input id="1">"10"</input>
            <output id="1">"11"</output>
            <input id="2">"0"</input>
            <output id="2">"1"</output>
            <input id="3">"111"</input>
            <output id="3">"1000"</output>
        </test>
    </inc>
    <add sign="3b">
        <test  sign="3b" grade="3">
            <input id="1">"10", "11"</input>
            <output id="1">"101"</output>
            <input id="2">"10", "0"</input>
            <output id="2">"10"</output>
            <input id="3">"111", "111"</input>
            <output id="3">"1110"</output>
        </test>
    </add>
    <pow_two sign="3c">
        <test  sign="3c1" grade="3">
            <input id="1">"10", 2</input>
            <output id="1">"1000"</output>
            <input id="2">"100001", 3</input>
            <output id="2">"100001000"</output>
        </test>
        <test  sign="3c2" grade="1">
            <input id="1">"0", 2</input>
            <output id="1">"0"</output>
            <input id="2">"101", 0</input>
            <output id="2">"101"</output>
        </test>
    </pow_two>
    <div_two sign="3d">
        <test  sign="3d1" grade="3">
            <input id="1">"100", 2</input>
            <output id="1">"1"</output>
            <input id="2">"111", 2</input>
            <output id="2">"1"</output>
        </test>
        <test  sign="3d2" grade="2">
            <input id="1">"111", 6</input>
            <output id="1">"0"</output>
            <input id="2">"100", 0</input>
            <output id="2">"100"</output>
            <input id="3">"1", 4</input>
            <output id="3">"0"</output>
            <input id="4">"1", 1</input>
            <output id="4">"0"</output>
        </test>
    </div_two>
    <leq sign="3e">
        <test  sign="3e1" grade="3">
            <input id="1">"100", "101"</input>
            <output id="1">True</output>
            <input id="2">"100", "0"</input>
            <output id="2">False</output>
            <input id="3">"100", "100"</input>
            <output id="3">True</output>
        </test>
        <test  sign="3e2" grade="1">
            <input id="1">"1", "0"</input>
            <output id="1">False</output>
            <input id="2">"0", "1"</input>
            <output id="2">True</output>
            <input id="3">"0", "0"</input>
            <output id="3">True</output>
        </test>
    </leq>
    <to_decimal sign="3f">
        <test  sign="3f1" grade="3">
            <input id="1">"100"</input>
            <output id="1">4</output>
            <input id="2">"1111"</input>
            <output id="2">15</output>
            <input id="3">"10101"</input>
            <output id="3">21</output>
        </test>
        <test  sign="3f2" grade="1">
            <input id="1">"0"</input>
            <output id="1">0</output>
            <input id="2">"1"</input>
            <output id="2">1</output>
        </test>
    </to_decimal>

    <divisors sign="5a">
        <test  sign="5a1" grade="4">
            <input id="1">6</input>
            <output id="1">[1,2,3]</output>
            <input id="2">97</input>
            <output id="2">[1]</output>
            <input id="3">36</input>
            <output id="3">[1,2,3,4,6,9,12,18]</output>
        </test>
        <test  sign="5a2" grade="1">
            <input id="1">1</input>
            <output id="1">[]</output>
        </test>
    </divisors>
    <perfect_numbers sign="5b">
        <test  sign="5b" grade="4">
            <input id="1">2</input>
            <output id="1">[6,28]</output>
            <input id="2">4</input>
            <output id="2">[6,28,496,8128]</output>
        </test>
    </perfect_numbers>
    <abundant_density sign="5c">
        <test  sign="5c" grade="3">
            <input id="1">20</input>
            <output id="1">0.15</output>
            <input id="2">32</input>
            <output id="2">0.15625</output>
            <input id="3">40</input>
            <output id="3">0.175</output>
        </test>
    </abundant_density>
    <semi_perfect_3 sign="5e">
        <test  sign="5e" grade="3">
            <input id="1">18</input>
            <output id="1">[3,6,9]</output>
            <input id="2">45</input>
            <output id="2">None</output>
            <input id="3">18</input>
            <output id="3">[3,6,9]</output>
        </test>
    </semi_perfect_3>

</functions>